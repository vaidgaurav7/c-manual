#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i,j;
    long long int s=0,ar[100];
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
      {
      scanf("%ld",&ar[i]);
                   
          }
    
    for(j=0;j<n;j++)
        {
                s+=ar[j];
        }
        printf("%ld",s);
    return 0;
}
